<!-- reference to the genre model (Enumerable is a list or collection) -->
@model IEnumerable<MvcMusicStore.Models.Genre>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Store";
}

<h2>Browse our Store</h2>

<ul>
    @foreach (var g in Model)
    {
        <li>@Html.ActionLink(g.Name, "Browse", "Store", new { genre=g.Name }, null)</li>
    }
</ul>



